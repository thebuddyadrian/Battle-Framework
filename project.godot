;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Sonic Battle Framework"
config/description="No Description"
run/main_scene="res://menus/mode_select.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
config/icon="uid://dcdb3dyga8r2u"
config/windows_native_icon="res://icon.ico"

[autoload]

Game="*res://game.gd"
Lists="*res://autoload/lists.gd"
MusicPlayer="*res://autoload/music_player.gd"
SceneChanger="*res://autoload/scene_changer.tscn"
Globals="*res://autoload/Globals.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
SoundEffectPlayer="*res://autoload/sound_effect_player.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[dialogic]

directories/dch_directory={
"Sonic": "res://assets/sonic/Sonic.dch"
}
directories/dtl_directory={
"INTRO": "res://DIALOGUE/CUTSCENE/INTRO.dtl"
}
layout/style_list=["res://DIALOGUE/CUTSCENE/MainStory.tres"]
layout/default_style="res://DIALOGUE/CUTSCENE/MainStory.tres"
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={}
glossary/default_case_sensitive=true

[display]

window/size/viewport_width=240
window/size/viewport_height=160
window/size/window_width_override=640
window/size/window_height_override=480
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[dotnet]

project/assembly_name="Sonic Battle Framework"

[editor_plugins]

enabled=PackedStringArray("res://addons/abfi/plugin.cfg", "res://addons/anthonyec.camera_preview/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

